{
    "Routes": [
        {
            "DownstreamPathTemplate": "/api/Event",
            "DownstreamScheme": "https",
            "DownstreamHostAndPorts": [
                {
                    "Host": "localhost",
                    "Port": 7015
                }
            ],
            "UpstreamPathTemplate": "/events",
            "UpstreamHttpMethod": [ "GET", "POST" ],
            "SwaggerKey": "events"
        },
        {
            "DownstreamPathTemplate": "/api/Event/{id}",
            "DownstreamScheme": "https",
            "DownstreamHostAndPorts": [
                {
                    "Host": "localhost",
                    "Port": 7015
                }
            ],
            "UpstreamPathTemplate": "/events/{id}",
            "UpstreamHttpMethod": [ "GET", "PUT", "DELETE" ],
            "SwaggerKey": "events"
        },
        {
            "DownstreamPathTemplate": "/api/Token",
            "DownstreamScheme": "https",
            "DownstreamHostAndPorts": [
                {
                    "Host": "localhost",
                    "Port": 7150
                }
            ],
            "UpstreamPathTemplate": "/auth/token",
            "UpstreamHttpMethod": [ "POST" ],
            "SwaggerKey": "auth",
            "AuthenticationOptions": {
                "AuthenticationProviderKey": "ApiKey"
            },
            "DelegatingHandlers": [
                "ApiKeyHandler"
            ]
        },
        {
            "DownstreamPathTemplate": "/swagger/{everything}",
            "DownstreamScheme": "https",
            "DownstreamHostAndPorts": [
                {
                    "Host": "localhost",
                    "Port": 7150
                }
            ],
            "UpstreamPathTemplate": "/auth-swagger/{everything}",
            "UpstreamHttpMethod": [ "GET" ],
            "SwaggerKey": "auth"
        }
    ],
    "SwaggerEndPoints": [
        {
            "Key": "events",
            "TransformByOcelotConfig": true,
            "Config": [
                {
                    "Name": "EventProvider API",
                    "Version": "v1",
                    "Url": "https://localhost:7015/swagger/v1/swagger.json"
                }
            ]
        },
        {
            "Key": "auth",
            "TransformByOcelotConfig": true,
            "Config": [
                {
                    "Name": "JWT Auth Provider API",
                    "Version": "v1",
                    "Url": "https://localhost:7150/swagger/v1/swagger.json"
                }
            ],
            "SecurityDefinitions": {
                "ApiKey": {
                    "Type": "apiKey",
                    "Name": "X-API-KEY",
                    "In": "header"
                }
            },
            "SecurityRequirements": {
                "ApiKey": []
            }
        }
    ],
    "GlobalConfiguration": {
        "BaseUrl": "https://localhost:7180"
    }
}